{
  "network": "testnet",
  "port": 3001,
  "services": [
    "insight-api",
    "bitcoind",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/lib/node_modules/bitcore/node_modules/bitcore-node/bin/bitcoind"
      }
    },
    "insight-api": {
      "routePrefix": "api",
      "cwdRequirePath": "node_modules/insight-api",
      "disableRateLimiter": true
    }
  }
}
